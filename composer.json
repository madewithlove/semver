{
	"name": "madewithlove/semver",
	"description": "A semver checker for Packagist",
	"license": "MIT",
	"authors": [
		{
			"name": "Hannes Van De Vreken",
			"email": "vandevreken.hannes@gmail.com"
		},
		{
			"name": "Maxime Fabre",
			"email": "ehtnam6@gmail.com"
		}
	],
	"require": {
		"composer/composer": "^1.0",
		"filp/whoops": "^2.1",
		"illuminate/cache": "^5.2",
		"illuminate/filesystem": "^5.2",
		"knplabs/packagist-api": "^1.3",
		"league/container": "^2.2",
		"league/plates": "^3.1",
		"league/route": "^2.0",
		"zendframework/zend-diactoros": "^1.3"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.16",
		"mockery/mockery": "^0.9.4",
		"phpunit/phpunit": "^5.3",
		"symfony/var-dumper": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Semver\\": "app/Semver"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Semver\\Unit\\": "tests/unit/Semver"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "5.6.20"
		}
	}
}
